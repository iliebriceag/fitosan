,\r\n        removeCart(product) {\r\n            const itemCart = this.products.filter((item) => item.id == product.id)[0];\r\n            itemCart.stock += product.cant;\r\n            let indiceProduct = 0;\r\n            this.cart.forEach((item, i) =>\r\n                item.id == product.id ? (indiceProduct = i) : null\r\n            );\r\n            this.cart.splice(indiceProduct, 1);\r\n        },\r\n        cleanCart() {\r\n            this.cart = [];\r\n        }\r\n    },\r\n    computed: {\r\n        cantProductsInCart() {\r\n            return this.cart.reduce((acc, item) => acc + item.cant, 0);\r\n        },\r\n        totalPriceCart() {\r\n            return this.cart.reduce((acc, item) => acc + item.cant * item.price, 0);\r\n        }\r\n    },\r\n    // methods: {\r\n    //     scrollToTop() {\r\n    //         window.scrollTo(0, 0)\r\n    //     }\r\n    // },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.link {\r\n    text-decoration: none;\r\n    color: white;\r\n    background-color: var(--primaryColor);\r\n    padding: 10px 20px;\r\n    border-radius: 30px;\r\n}\r\n\r\n.link:hover {\r\n    background-color: #30694bd3;\r\n}\r\n\r\n.page-cards {\r\n    margin-top: 20px;\r\n    padding-top: 80px;\r\n    padding-bottom: 80px;\r\n}\r\n\r\n.page-cards .card-text {\r\n    height: 55px;\r\n    -webkit-box-orient: vertical;\r\n    display: block;\r\n    display: -webkit-box;\r\n    font-family: sans-serif;\r\n    -webkit-line-clamp: 2;\r\n    overflow: hidden;\r\n    text-overflow: ellipsis;\r\n}\r\n\r\n.page-cards .card {\r\n    display: block;\r\n    background-color: whitesmoke;\r\n    color: rgb(150, 150, 150);\r\n    border: none;\r\n    border-radius: 50px;\r\n    box-shadow: 0 3px 6px rgba(0, 0, 0, 0.12), 0 1px 2px rgba(0, 0, 0, 0.24);\r\n}\r\n\r\n.page-cards .card:hover {\r\n    box-shadow: var(--primaryColor) 0px 5px 15px;\r\n}\r\n\r\n.page-cards .card .card-body img {\r\n    max-width: 180px;\r\n}\r\n\r\n.more {\r\n    display: block;\r\n    mar