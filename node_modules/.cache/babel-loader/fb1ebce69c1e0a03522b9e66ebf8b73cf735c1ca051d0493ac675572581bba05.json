rt(product) {\n      const itemCart = this.products.filter(item => item.id == product.id)[0];\n      itemCart.stock += product.cant;\n      let indiceProduct = 0;\n      this.cart.forEach((item, i) => item.id == product.id ? indiceProduct = i : null);\n      this.cart.splice(indiceProduct, 1);\n    },\n    cleanCart() {\n      this.cart = [];\n    }\n  },\n  computed: {\n    cantProductsInCart() {\n      return this.cart.reduce((acc, item) => acc + item.cant, 0);\n    },\n    totalPriceCart() {\n      return this.cart.reduce((acc, item) => acc + item.cant * item.price, 0);\n    }\n  }\n  // methods: {\n  //     scrollToTop() {\n  //         window.scrollTo(0, 0)\n  //     }\n  // },\n};","map":{"version":3,"names":["products","id","name","price","moneda","stock","data","cart","user","dinero","created","fetchApi","methods","dataApi","localStorage","getItem","JSON","parse","setItem","stringify","addCart","product","itemCart","filter","item","undefined","cant","push","removeCart","indiceProduct","forEach","i","splice","cleanCart","computed","cantProductsInCart","reduce","acc","totalPriceCart"],"sources":["D:\\My Projects\\sere-prj\\src\\components\\PageCardsProduct.vue"],"sourcesContent":["<template>\r\n    <div class=\"page-cards\">\r\n        <div class=\"container-fluid\">\r\n            <h1 class=\"p-5 text-center\">Produse</h1>\r\n            <div class=\"row row-cols-1 row-cols-md-2 row-cols-lg-4 info-cards\">\r\n                <div class=\"col p-2\" v-for=\"item in cart\" :key=\"item.id\">\r\n                    <div class=\"card\">\r\n                        <div class=\"card-body text-center\">\r\n                            <h5 class=\"card-title p-2 text-black\">{{ item.name }}</h5>\r\n                            <h1 class=\"card-title p-2\"><img :src=\"item.imgSrc\" alt=\"handshake\"></h1>\r\n                            <p class=\"card-text p-2\">{{ item.description }}</p>\r\n                            <p class=\"text-black\" :class=\"{ 'not-in-stock': product.inStock === 0 }\">Preț: <strong>{{\r\n                                item.price }}</strong> RON <span class=\"me-5\"></span> Stock: <strong>{{ item.stock\r\n                                    }}</strong> </p>\r\n                            <button class=\"link\"\r\n                                @click.prevent=\"addProdToCart(product)\"><i class=\"bi bi-cart2 me-2\"></i>Adaugă în\r\n                                coș</button>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <a class=\"mt-5 more d-flex justify-content-center align-items-center\" href=\"#\">Vezi mai multe..</a>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nconst products = [\r\n    {\r\n        id: 1,\r\n        name: \"Empanada de Carne\",\r\n        price: 200,\r\n        moneda: \"$\",\r\n        stock: 5\r\n    },\r\n    {\r\n        id: 2,\r\n        name: \"Chinchulines\",\r\n        price: 200,\r\n        moneda: \"$\",\r\n        stock: 5\r\n    },\r\n    {\r\n        id: 3,\r\n        name: \"Milanesas\",\r\n        price: 300,\r\n        moneda: \"$\",\r\n        stock: 5\r\n    },\r\n    {\r\n        id: 4,\r\n        name: \"Locro\",\r\n        price: 500,\r\n        moneda: \"$\",\r\n        stock: 5\r\n    },\r\n    {\r\n        id: 5,\r\n        name: \"Pizza\",\r\n        price: 500,\r\n        moneda: \"$\",\r\n        stock: 5\r\n    },\r\n    {\r\n        id: 6,\r\n        name: \"Fainá\",\r\n        price: 400,\r\n        moneda: \"$\",\r\n        stock: 5\r\n    },\r\n    {\r\n        id: 7,\r\n        name: \"Polenta\",\r\n        price: 200,\r\n        moneda: \"$\",\r\n        stock: 5\r\n    },\r\n    {\r\n        id: 8,\r\n        name: \"Ñoquis\",\r\n        price: 200,\r\n        moneda: \"$\",\r\n        stock: 5\r\n    },\r\n    {\r\n        id: 9,\r\n        name: \"Ravioles\",\r\n        price: 300,\r\n        moneda: \"$\",\r\n        stock: 5\r\n    },\r\n    {\r\n        id: 10,\r\n        name: \"Empanada de pollo\",\r\n        price: 200,\r\n        moneda: \"$\",\r\n        stock: 5\r\n    },\r\n    {\r\n        id: 11,\r\n        name: \"Empanada de Jamon y Queso\",\r\n        price: 200,\r\n        moneda: \"$\",\r\n        stock: 5\r\n    }\r\n];\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            products: [],\r\n            cart: [],\r\n            user: {\r\n                name: \"user\",\r\n                dinero: 1000\r\n            }\r\n        }\r\n    },\r\n    created() {\r\n        this.fetchApi();\r\n    },\r\n    methods: {\r\n        fetchApi() {\r\n            const dataApi = localStorage.getItem(\"db\");\r\n            if (dataApi != null) {\r\n                this.products = JSON.parse(dataApi);\r\n            } else {\r\n                this.products = products;\r\n                localStorage.setItem(\"db\", JSON.stringify(data.products));\r\n            }\r\n        },\r\n        addCart(product) {\r\n            const itemCart = this.cart.filter((item) => item.id == product.id)[0];\r\n            if (itemCart != undefined) {\r\n                itemCart.cant++;\r\n            } else {\r\n                const itemCart = {\r\n                    id: product.id,\r\n                    name: product.name,\r\n                    price: product.price,\r\n                    moneda: product.moneda,\r\n                    cant: 1\r\n                };\r\n                this.cart.push(itemCart);\r\n            }\r\n            product.stock--;\r\n        },\r\n        removeCart(product) {\r\n            const itemCart = this.products.filter((item) => item.id == product.id)[0];\r\n            itemCart.stock += product.cant;\r\n            let indiceProduct = 0;\r\n            this.cart.forEach((item, i) =>\r\n                item.id == product.id ? (indiceProduct = i) : null\r\n            );\r\n            this.cart.splice(indiceProduct, 1);\r\n        },\r\n        cleanCart() {\r\n            this.cart = [];\r\n        }\r\n    },\r\n    computed: {\r\n        cantProductsInCart() {\r\n            return this.cart.reduce((acc, item) => acc + item.cant, 0);\r\n        },\r\n        totalPriceCart() {\r\n            return this.cart.reduce((acc, item) => acc + item.cant * item.price, 0);\r\n        }\r\n    },\r\n    // methods: {\r\n    //     scrollToTop() {\r\n    //         window.scrollTo(0, 0)\r\n    //     }\r\n    // },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.link {\r\n    text-decoration: none;\r\n    color: white;\r\n    background-color: var(--primaryColor);\r\n    padding: 10px 20px;\r\n    border-radius: 30px;\r\n}\r\n\r\n.link:hover {\r\n    background-color: #30694bd3;\r\n}\r\n\r\n.page-cards {\r\n    margin-top: 20px;\r\n    padding-top: 80px;\r\n    padding-bottom: 80px;\r\n}\r\n\r\n.page-cards .card-text {\r\n    height: 55px;\r\n    -webkit-box-orient: vertical;\r\n    display: block;\r\n    display: -webkit-box;\r\n    font-family: sans-serif;\r\n    -webkit-line-clamp: 2;\r\n    overflow: hidden;\r\n    text-overflow: ellipsis;\r\n}\r\n\r\n.page-cards .card {\r\n    display: block;\r\n    background-color: whitesmoke;\r\n    color: rgb(150, 150, 150);\r\n    border: none;\r\n    border-radius: 50px;\r\n    box-shadow: 0 3px 6px rgba(0, 0, 0, 0.12), 0 1px 2px rgba(0, 0, 0, 0.24);\r\n}\r\n\r\n.page-cards .card:hover {\r\n    box-shadow: var(--primaryColor) 0px 5px 15px;\r\n}\r\n\r\n.page-cards .card .card-body img {\r\n    max-width: 180px;\r\n}\r\n\r\n.more {\r\n    display: block;\r\n    margin: auto;\r\n    border: 2px solid var(--primaryColor);\r\n    color: var(--primaryColor);\r\n    text-decoration: none;\r\n    border-bottom-left-radius: 30px;\r\n    border-bottom-right-radius: 30px;\r\n    width: 200px;\r\n    height: 40px;\r\n}\r\n\r\n.more:hover {\r\n    background-color: var(--primaryColor);\r\n    color: white;\r\n    transition: all .3s;\r\n}\r\n</style>\r\n"],"mappings":";AA2BA,MAAMA,QAAO,GAAI,CACb;EACIC,EA